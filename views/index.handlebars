 <h2 class="page-header">Dashboard</h2>
<p>Welcome to your dashboard</p>

<div>
       {{# each items }}
           <article class="item">
              {{ this.item}}
           </article>
       {{/each}}
   </div>

<form method="get" action="/users/addWebsite">
  <div class="form-group">
    <label>Add website here </label>
    <input type="text" class="form-control" name="website" placeholder="website name">
  </div>
  <div class="form-group">
    <label>Enter your username</label>
    <input type="text" class="form-control" name="usname" placeholder="User name">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>
